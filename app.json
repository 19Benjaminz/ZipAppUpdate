{
  "expo": {
    "name": "ZipAppTwo",
    "slug": "ZipAppTwo",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/zipcodexpress_square.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.unibox.express",
      "buildNumber": "2.0.1",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.zipcodexpress1",
      "versionCode": 4,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/ZipporaLogo-square@3x.png",
          "ios": {
            "image": "./assets/images/SplashScreen_1125x2436.png",
            "enableFullScreenImage_legacy": true
          },
          "android": {
            "image": "./assets/images/ZipporaLogo-square.png",
            "resizeMode": "contain",
            "imageWidth": "200"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d5c4de98-003a-4393-9380-db9d958ed8e2"
      }
    }
  }
}
